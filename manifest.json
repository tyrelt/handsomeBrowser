{
  "manifest_version": 2,
  "name": "Handsome Browser",
  "description": "Men, you've found your home online.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
      {
        "matches": ["http://*/*"],
        "js": ["handsomeBrowser.js"]
      }
  ]
}